  <script>
  function er(){
    document.getElementById("inf1").innerHTML ="";
var i = 20;
while (i < 36) { 
  document.getElementById("inf1").innerHTML =document.getElementById("inf1").innerHTML +i +"<br>"
  i++;
}
}
 function er1() {
   document.getElementById("inf2").innerHTML ="";
      var b = Number(document.getElementById("var_in").value);
      i=10;
for (;i<=b;){
document.getElementById("inf2").innerHTML =document.getElementById("inf2").innerHTML + i *i +"<br>";
i++;
}
}
function er2() {
   document.getElementById("inf3").innerHTML ="";
      var a = Number(document.getElementById("var_in1").value);
      i=a;
for (;i<=50;){
document.getElementById("inf3").innerHTML =document.getElementById("inf3").innerHTML + i *i *i +"<br>";
i++;
}
}
function er3() {
   document.getElementById("inf4").innerHTML ="";
      var a = Number(document.getElementById("var_in2").value);
      var b = Number(document.getElementById("var_in3").value);
      i=a;
for (;i<=b;){
document.getElementById("inf4").innerHTML =document.getElementById("inf4").innerHTML +i +"<br>";
i++;
}
}
</script>
    
    
    
    
